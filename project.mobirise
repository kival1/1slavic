        {
          "_name": "about",
          "_sourceTheme": "innovationamp",
          "_customHTML": "<section class=\"about1\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <header>Image</header>\n            <input type=\"range\" inline=\"\" title=\"Image Height\" name=\"imgHeight\" min=\"300\" max=\"700\" step=\"50\" value=\"500\">\n            <input type=\"checkbox\" title=\"Image Effect\" name=\"imgEffect\">\n            <input type=\"color\" title=\"Image Overlay Color\" name=\"imgOverlayColor1\" value=\"#fa584d\" condition=\"imgEffect\">\n            <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"0.9\" step=\"0.05\" value=\"0.35\" condition=\"imgEffect\">\n        <header>Text</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Button\" name=\"showButtons\" checked=\"\">\n         <header>Background</header>   \n        <input type=\"color\" title=\"Background Color\" value=\"#74f4d1\" name=\"bgColor\">\n    </mbr-parameters>\n\n   <div class=\"container\">\n        <div class=\"mbr-row wrapper rounded__10\">\n            <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-6 mbr-px-0\">\n                <div class=\"image\">\n                    <div class=\"mbr-overlay rounded__10\"></div>\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/154890-ovctsd-863-1108x1108.jpeg\" alt=\"image\" height=\"500\" width=\"688.1987577639752\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                       <img src=\"@PROJECT_PATH@/assets/images/154890-ovctsd-863-1108x1108.jpeg\" class=\"mbr-temp\" alt=\"\" data-aspectratio=\"0.7265342960288809\">\n                    </amp-img>\n                </div>\n                </div>\n            <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-6 mbr-flex mbr-px-0\">\n                <div class=\"text__block align-left mbr-m-auto\">\n                    <h3 mbr-if=\"showTitle\" class=\"mbr-fonts-style card__title mbr-section-title mbr-bold mbr-black\" mbr-theme-style=\"display-5\" data-app-selector=\".card__title\">Пожертвование</h3>\n                   <p class=\"mbr-fonts-style card__text\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-section-btn, .card__text\">Ваше пожертвование пойдет на развитие служения церкви. Размер пожертвований зависит от расположения сердца. Пожертвования также являются семенем, которое сеется в Божье Царство и в соответствии с Писанием оно всегда приносит урожай.</p>\n                    <div class=\"mbr-section-btn mbr-pt-4\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showButtons\"><a class=\"btn btn-md btn-secondary\" href=\"https://give.cornerstone.cc/fsebc\" data-app-placeholder=\"Type Text\">\n                            \n                            <span class=\"btn__hover\">пожертвовать</span>\n                        </a></div>\n               </div>\n           </div>\n            \n        </div>\n    </div>\n</section>",
          "_styles": {
            "padding-top": "(@paddingTop * 10px)",
            "padding-bottom": "(@paddingBottom * 10px)",
            "background-color": "@bgColor",
            ".mbr-row": {
              "margin": "0"
            },
            ".wrapper": {
              "box-shadow": "0 0 35px 0 rgba(0.5, 0.5, 0.5, 0.5)",
              "border-radius": "30px"
            },
            ".image": {
              "height": "(@imgHeight * 1px)",
              "min-height": "200px",
              "width": "100%",
              "position": "relative",
              "amp-img, img": {
                "border-top-right-radius": "0",
                "border-bottom-right-radius": "0",
                "border-radius": "30px",
                "& when (@imgEffect)": {
                  "filter": "grayscale(100%)"
                }
              }
            },
            ".mbr-px-0": {
              "padding-left": "0",
              "padding-right": "0"
            },
            "& when (@imgEffect)": {
              ".mbr-overlay": {
                "opacity": "@imgOverlayOpacity",
                "z-index": "1",
                "pointer-events": "none",
                "background-color": "@imgOverlayColor1",
                "border-top-right-radius": "0",
                "border-bottom-right-radius": "0"
              }
            },
            ".card__title": {
              "margin-bottom": "20px",
              "text-align": "center"
            },
            ".text__block": {
              "padding": "50px 15px 50px 60px"
            },
            "@media (max-width: 768px)": {
              ".image": {
                "height": "300px",
                "amp-img, img": {
                  "border-top-left-radius": "10px",
                  "border-top-right-radius": "10px",
                  "border-bottom-left-radius": "0"
                }
              },
              ".text__block": {
                "padding": "50px 15px 50px 15px"
              },
              ".mbr-overlay": {
                "border-top-left-radius": "10px",
                "border-top-right-radius": "10px",
                "border-bottom-left-radius": "0"
              }
            },
            ".mbr-section-btn, .card__text": {
              "text-align": "center"
            }
          },
